{
  "name": "react-redux-starter",
  "version": "0.1.0",
  "description": "A boiler-plate setup for React/Redux applications",
  "engines": {
    "node": "4.x"
  },
  "scripts": {
    "postinstall": "npm run clean; npm run build;",
    "build": "npm run clean; NODE_ENV=production webpack -p",
    "clean": "rimraf dist coverage",
    "cover": "istanbul cover -x '*.test.js' _mocha -- --opts ./tests/mocha.opts tests/**/*.js src/**/*.js && istanbul check-coverage --lines=69",
    "dev": "NODE_ENV=development nodemon server.js",
    "lint": "eslint .",
    "test": "npm run lint && mocha --opts ./tests/mocha.opts && npm run cover",
    "test:watch": "mocha --opts ./tests/mocha.opts --watch",
    "start": "NODE_ENV=production node server.js"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rangle/react-redux-starter.git"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "babel": "^6.3.26",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "basscss": "^7.0.4",
    "chai": "^3.4.0",
    "chalk": "^1.1.1",
    "css-loader": "^0.23.0",
    "cssnext-loader": "^1.0.1",
    "eslint": "^1.8.0",
    "eslint-loader": "^1.1.1",
    "eslint-plugin-react": "^3.6.3",
    "express": "^4.13.3",
    "file-loader": "^0.8.4",
    "history": "^1.17.0",
    "html-webpack-plugin": "^1.7.0",
    "http-proxy": "^1.12.1",
    "immutable": "^3.7.5",
    "istanbul": "^0.4.0",
    "jsdom": "^7.0.2",
    "json-loader": "^0.5.3",
    "mocha": "^2.3.3",
    "nodemon": "^1.8.1",
    "postcss-loader": "^0.8.0",
    "radium": "^0.16.5",
    "react": "^0.14.0",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.2",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.0-rc5",
    "react-router-redux": "^2.1.0",
    "redux": "^3.0.4",
    "redux-form": "^4.1.6",
    "redux-localstorage": "^0.4.0",
    "redux-logger": "^2.1.4",
    "redux-thunk": "^1.0.0",
    "require-hacker": "^2.1.2",
    "rimraf": "^2.4.3",
    "serve-webpack-client": "^0.0.4",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "winston": "^2.1.1"
  }
}
