  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p",
    "clean": "rimraf gh-pages/ coverage/",
    "cover": "nyc check-coverage --lines 100 --functions 100 --branches 100",
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info",
    "lint": "npm run lint-js",
    "lint-js": "eslint './src/**/*.js'",
    "start": "cross-env NODE_ENV=production http-server ./gh-pages/",
    "pretest": "npm run clean && npm run lint",
    "test": "nyc --reporter=lcov --reporter=text-summary mocha",
    "posttest": "npm run cover",
    "test:watch": "mocha --watch",
    "lib": "webpack --config webpack.lib.config.js",
    "prepublish": "npm run lib",
    "deploy": "gh-pages -d gh-pages"
  },
  "nyc": {
    "exclude": [
      "**/*.test.js",
      "test/entry.js",
      "server/*",
      "dist/*"
    ]
  },
